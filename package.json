{
  "name": "react-redux-boilerplate",
  "version": "0.0.1",
  "description": "Custom React boilerplate with TypeScript and Redux-Observable",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vostack13/react-redux-boilerplate.git"
  },
  "keywords": [
    "react",
    "redux",
    "rxjs",
    "redux-observable",
    "emotion",
    "typescript"
  ],
  "author": "Stanislav Vozikov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vostack13/react-redux-boilerplate/issues"
  },
  "homepage": "https://github.com/vostack13/react-redux-boilerplate#readme",
  "scripts": {
    "clean-dist": "rm -f -r -d dist",
    "dev": "webpack-dev-server --config=config/webpack.config.js --no-inline",
    "start": "better-npm-run start",
    "build": "npm run clean-dist && better-npm-run build"
  },
  "betterScripts": {
    "build": {
      "command": "webpack -p --config=config/webpack.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start": {
      "command": "webpack-dev-server --config=config/webpack.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "dependencies": {
    "@emotion/core": "^10.0.27",
    "@emotion/styled": "^10.0.27",
    "axios": "^0.19.2",
    "emotion-theming": "^10.0.27",
    "js-cookie": "^2.2.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-observable": "^1.2.0",
    "reselect": "^4.0.0",
    "rxjs": "^6.5.4"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@svgr/webpack": "^5.1.0",
    "@types/js-cookie": "^2.2.4",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.3",
    "babel-loader": "^8.0.6",
    "better-npm-run": "^0.1.1",
    "copy-webpack-plugin": "^5.1.1",
    "html-webpack-plugin": "^3.2.0",
    "terser-webpack-plugin": "^2.3.4",
    "ts-loader": "^6.2.1",
    "tslint": "^6.0.0",
    "typescript": "^3.7.5",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.2"
  }
}
